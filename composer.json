{
	"name": "petak23/iot-server",
	"description": "IoT aplikácia na ESP32/ESP8266 a serverová aplikácia k tomu",
	"keywords": ["nette", "IoT-server"],
	"type": "project",
	"license": ["MIT", "BSD-3-Clause", "GPL-2.0", "GPL-3.0"],
	"authors": [
		{
			"name": "Petr Brouzda",
			"homepage": "https://pebrou.wordpress.com"
		},
		{
			"name": "Peter Vojtech",
			"homepage": "http://petak23.echo-msz.eu"
		}
	],
	"require": {
		"php": ">=8.0",
		"nette/application": "^3.1",
		"nette/bootstrap": "^3.0",
		"nette/caching": "^3.0",
		"nette/database": "^3.0",
		"nette/di": "^3.0",
		"nette/finder": "^2.5",
		"nette/forms": "^3.0",
		"nette/http": "^3.0",
		"nette/mail": "^3.0",
		"nette/robot-loader": "^3.0",
		"nette/security": "^3.0",
		"nette/utils": "^3.2",
		"latte/latte": "^2.10",
		"tracy/tracy": "^2.9",
		"mdanter/ecc": "^1.0",
		"dg/adminer-custom": "^1.24",
		"contributte/webpack": "^2.1"
	},
	"require-dev": {
		"nette/tester": "^2.4",
		"symfony/thanks": "^1"
	},
	"autoload": {
		"classmap": ["app/Bootstrap.php"]
	},
	"minimum-stability": "stable",
	"config": {
		"allow-plugins": {
			"symfony/thanks": true
		}
	}
}
